    "import time\n",
    "\n",
    "def timer_test(func):\n",
    "    def times():\n",
    "        start = time.time()\n",
    "        func()\n",
    "        end = time.time()\n",
    "        print(end - start)\n",
    "    return times    \n",
    "\n",
    "\n",
    "@timer_test\n",
    "def test2():\n",
    "    print(\"Virat\")\n",
    "    \n",
    "    \n",
    "test2()    \n"
   ]
